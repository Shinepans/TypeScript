>rawr : (dino: "t-rex" | "raptor") => string
>dinosaur : "t-rex" | "raptor"

function rawr(dino: RexOrRaptor) {
>rawr : (dino: "t-rex" | "raptor") => string
>dino : "t-rex" | "raptor"
>RexOrRaptor : "t-rex" | "raptor"

    if (dino === "t-rex") {
>dino === "t-rex" : boolean
>dino : "t-rex" | "raptor"
>"t-rex" : "t-rex"

        return "ROAAAAR!";
>"ROAAAAR!" : "ROAAAAR!"
    }
    if (dino === "raptor") {
>dino === "raptor" : boolean
>dino : "t-rex" | "raptor"
>"raptor" : "raptor"

        return "yip yip!";
>"yip yip!" : "yip yip!"
    }

    throw "Unexpected " + dino;
